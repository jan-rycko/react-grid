{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ],
    "moduleResolution": "node",
    "preserveConstEnums": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "src": [ "src" ]
    }
  }
}